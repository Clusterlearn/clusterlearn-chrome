{
    "name": "test",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "test",
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "index.html"
    },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.udemy.com/cart/subscribe/course/*/.*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "index.html"
  }
}